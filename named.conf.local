zone "binarybutter.com" {
  type master;
  file "/etc/bind/zones/binarybutter.com.db";
  allow-update {
    key "binarybutter.com.";
  };
};

zone "3.2.1.in-addr.arpa" {
  type master;
  file "/etc/bind/zones/rev.3.2.1.in-addr.arpa";
};

key "binarybutter.com." {
  algorithm hmac-md5;
  secret "TEMPLATED_DNS_UPDATES_SECRET_KEY";
};
